<!--Inicio Modal Nuevo Almacén-->
<div id="modal-nuevo" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Nuevo Almacén
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="NombreAlmacen" name="NombreAlmacen" type="text" placeholder="Ingrese Nombre Almacén" class="form-control" />
                    </div>
                    <div class="form-group">
                        <input id="DireccionAlmacen" name="DireccionAlmacen" type="text" placeholder="Ingrese Dirección Almacén" class="form-control" />
                    </div>
                    <div class="form-group">
                        <input id="idSucursal" type="hidden" class="form-control" />
                        <input id="nombreSucursal" type="hidden" class="form-control" />
                        <input onkeyup="buscarSucursal(event)" id="sucursalNuevo" type="text" placeholder="Ingrese Nombre Sucursal" class="form-control" autocomplete="off" />
                        <div class="divPredictivo" id="divPredictivoSucursal">
                            <div class="divPredictivob">
                                <div class="divPredictivoa">
                                    <ul class="ulPredictivo" role="listbox" id="ulPredictivoSucursalJRM"></ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="checkbox" style="padding-left:20px;">
                            <label>
                                <input tabindex="5" type="checkbox" id="IsPrincipal" value="S" />&nbsp; ¿Es almacén principal?
                            </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpAlmacen" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="agregarAlmacen()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Nuevo Almacén-->
