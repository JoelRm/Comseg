<!--Inicio Modal Editar-->
<div id="modalEditarTipoPersona" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Editar TipoPersona
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="idTipoPersonaEditar" name="idTipoPersonaEditar" type="hidden" placeholder="Ingrese Nombre TipoPersona" class="form-control" />
                        <input id="NombreTipoPersonaEditar" name="NombreTipoPersonaEditar" type="text" placeholder="Ingrese Nombre TipoPersona" class="form-control" autocomplete="off"/>
                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpTipoPersona" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="editarTipoPersona()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Editar-->
