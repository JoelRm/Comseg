<!--Inicio Modal Editar-->
<div id="modalEditarLinea" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Editar Linea
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="idLineaEditar" name="idLineaEditar" type="hidden" placeholder="Ingrese Nombre Linea" class="form-control" />
                        <input id="NombreLineaEditar" name="NombreLineaEditar" type="text" placeholder="Ingrese Nombre Linea" class="form-control"  autocomplete="off"/>
                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpLinea" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="editarLinea()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Editar-->
