<!--Inicio Modal Editar-->
<div id="modalEditarMoneda" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Editar Moneda
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="idMonedaEditar" name="idMonedaEditar" type="hidden" placeholder="Ingrese Nombre Moneda" class="form-control" />
                        <input id="NombreMonedaEditar" name="NombreMonedaEditar" type="text" placeholder="Ingrese Nombre Moneda" class="form-control"  autocomplete="off"/>

                    </div>
                    <div class="form-group">
                        <input id="SimboloMonedaEditar" name="SimboloMonedaEditar" type="text" placeholder="Ingrese Simbolo Moneda" class="form-control"  autocomplete="off"/>
                    </div>


                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpEditarMoneda" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="editarMoneda()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Editar-->
