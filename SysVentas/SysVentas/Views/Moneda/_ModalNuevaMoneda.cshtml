<!--Inicio Modal Nueva Moneda-->
<div id="modalnuevoMoneda" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Nueva Moneda
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="NombreMoneda" name="NombreMoneda" type="text" placeholder="Ingrese Nombre Moneda" class="form-control"  autocomplete="off"/>
                    </div>
                    <div class="form-group">
                        <input id="SimboloMoneda" name="SimboloMoneda" type="text" placeholder="Ingrese Simbolo Moneda" class="form-control"  autocomplete="off"/>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpNuevaMoneda" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="agregarMoneda()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Nueva Moneda-->
