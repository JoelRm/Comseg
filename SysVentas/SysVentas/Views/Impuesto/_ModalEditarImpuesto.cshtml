<!--Inicio Modal Editar-->
<div id="modalEditarImpuesto" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Editar Impuesto
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="idImpuestoEditar" name="idImpuestoEditar" type="hidden" placeholder="Ingrese Nombre Impuesto" class="form-control" />
                        <input id="NombreImpuestoEditar" name="NombreImpuestoEditar" type="text" placeholder="Ingrese Nombre Impuesto" class="form-control"  autocomplete="off"/>
                    </div>
                    <div class="form-group">
                        <input id="ImpuestoEditar" name="ImpuestoEditar" type="text" placeholder="Ingrese Número de Impuesto" class="form-control"  autocomplete="off"/>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpImpuesto" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="editarImpuesto()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Editar-->
