<!--Inicio Modal Nueva Impuesto-->
<div id="modalNuevoImpuesto" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Nueva Impuesto
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal">
                    <div class="form-group">
                        <input id="NombreImpuesto" name="NombreImpuesto" type="text" placeholder="Ingrese Nombre Impuesto" class="form-control"  autocomplete="off"/>
                    </div>
                    <div class="form-group">
                        <input id="Impuesto" name="Impuesto" type="text" placeholder="Ingrese Número de Impuesto" class="form-control"  autocomplete="off"/>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpImpuesto" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="agregarImpuesto()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Nueva Impuesto-->
