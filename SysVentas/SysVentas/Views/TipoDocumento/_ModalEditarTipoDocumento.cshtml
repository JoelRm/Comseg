<!--Inicio Modal Editar-->
<div id="modalEditarTipoDocumento" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Editar TipoPersona
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal row">
                    <div class="form-group col-md-12">
                        <input id="idTipoDocumentoEditar" name="idTipoDocumentoEditar" type="hidden" placeholder="Ingrese Nombre TipoPersona" class="form-control" />
                        <input id="DescripcionTipoDocumentoEditar" name="DescripcionTipoDocumentoEditar" type="text" placeholder="Descripcion Nombre Tipo Documento" class="form-control" autocomplete="off" />
                    </div>
                    <div class="form-group col-md-6">
                        <input id="AbreviacionTipoDocumentoEditar" name="AbreviacionTipoDocumentoDescripcion" type="text" placeholder="Ingrese Abreviación Tipo Doc." class="form-control" autocomplete="off" />
                    </div>
                    <div class="form-group col-md-6">
                        <input id="LongitudTipoDocumentoEditar" name="LongitudTipoDocumentoDescripcion" type="text" placeholder="Ingrese Longitud Tipo Doc." class="form-control" autocomplete="off" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpTipoDocumentoEditar" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="editarTipoDocumento()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Editar-->
