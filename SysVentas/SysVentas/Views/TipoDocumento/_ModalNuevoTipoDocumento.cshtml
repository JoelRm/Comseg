<!--Inicio Modal Nueva TipoPersona-->
<div id="modalnuevoTipoDocumento" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" data-dismiss="modal" aria-hidden="true" class="close">
                    &times;
                </button>
                <h4 class="modal-title">
                    Nuevo Tipo de Documento
                </h4>
            </div>
            <div class="modal-body">
                <div class="form-body pal row">
                    <div class="form-group col-md-12">
                        <input id="DescripcionTipoDocumento" name="DescripcionTipoDocumento" type="text" placeholder="Ingrese Descripción Tipo Doc." class="form-control" autocomplete="off" />
                    </div>
                    <div class="form-group col-md-6">
                        <input id="AbreviacionTipoDocumento" name="AbreviacionTipoDocumento" type="text" placeholder="Ingrese Abreviación Tipo Doc." class="form-control" autocomplete="off" />
                    </div>
                    <div class="form-group col-md-6">
                        <input id="LongitudTipoDocumento" name="LongitudTipoDocumento" type="text" placeholder="Ingrese Longitud Tipo Doc." class="form-control" autocomplete="off" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" id="CerrarPopUpNuevoTipoDocumento" data-dismiss="modal" class="btn btn-danger">
                    Cancelar
                </button>
                <button type="button" onclick="agregarTipoDocumento()" class="btn btn-primary">
                    Guardar
                </button>
            </div>
        </div>
    </div>
</div>
<!--Fin Modal Nueva TipoPersona-->
